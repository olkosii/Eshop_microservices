        volume: {}
  discount.grpc:
    build:
      context: C:\Work\Study\Eshop_microservices\EShop
      dockerfile: src/Services/Discount/Discount.Grpc/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
